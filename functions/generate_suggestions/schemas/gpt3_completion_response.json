{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["choices", "created", "id", "model", "object"],
  "properties": {
    "choices": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "required": ["finish_reason", "index", "logprobs", "text"],
          "properties": {
            "finish_reason": {
              "type": "string"
            },
            "index": {
              "type": "integer"
            },
            "logprobs": {
              "type": "null"
            },
            "text": {
              "type": "string"
            }
          }
        }
      ]
    },
    "created": {
      "type": "integer"
    },
    "id": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "object": {
      "type": "string"
    }
  }
}
